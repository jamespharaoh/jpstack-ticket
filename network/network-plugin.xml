<plugin
	name="network"
	package="wbs.sms.network">

	<plugin-dependencies>
		<plugin-dependency name="sms-core"/>
	</plugin-dependencies>

	<sql-scripts>
		<sql-schema name="network"/>
	</sql-scripts>

	<models>
		<model name="network"/>
		<model name="networkPrefix"/>
	</models>

	<layer name="logic">
		<component class="logic.NetworkPrefixCache"/>
	</layer>

	<fixtures>
		<fixture name="network"/>
	</fixtures>

	<console-modules>
		<console-module name="network"/>
		<console-module name="network-prefix"/>
	</console-modules>

</plugin>
