<console-module
	name="batch-subject">

	<console-helper-provider
		object-name="batchSubject"
		view-priv="parent:">

		<priv-key
			name="batchSubject.manage"
			priv="parent:manage"/>

		<context-stuff
			name="batchSubjectName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="batchSubject"
		object-name="batchSubject">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="batch-subject"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
