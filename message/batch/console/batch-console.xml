<console-module
	name="batch">

	<console-helper-provider
		object-name="batch"
		view-priv="parent:">

		<priv-key
			name="batch.manage"
			priv="parent:manage"/>

		<context-stuff
			name="batchSubjectName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="batch"
		object-name="batch">

		<object>

			<object-summary-page
				form="summary"/>

			<object-sms-messages-page/>

			<object-sms-stats-page/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="batch"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
