<record
	name="inboxAttempt"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="inbox"
			cacheable="yes"/>

		<index-field
			cacheable="yes"/>

		<timestamp-field
			name="timestamp"
			column-type="iso"/>

		<!-- data -->

		<enum-field
			name="result"
			type="inboxState"/>

		<string-field
			name="statusMessage"
			nullable="yes"/>

		<reference-field
			type="exceptionLog"
			nullable="yes"/>

	</fields>

</record>