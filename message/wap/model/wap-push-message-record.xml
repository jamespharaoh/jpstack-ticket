<record
	name="wapPushMessage"
	type="minor">

	<fields>

		<!-- id -->

		<foreign-id-field
			field="message"/>

		<!-- identity -->

		<master-field
			type="message"/>

		<!-- data -->

		<reference-field
			name="textText"
			type="text"/>

		<reference-field
			name="urlText"
			type="text"/>

	</fields>

</record>