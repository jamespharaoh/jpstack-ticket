<record
	name="messageStatsData"
	type="composite">

	<fields>

		<integer-field
			name="inTotal"
			default="0"/>

		<integer-field
			name="outTotal"
			default="0"/>

		<integer-field
			name="outPending"
			default="0"/>

		<integer-field
			name="outCancelled"
			default="0"/>

		<integer-field
			name="outFailed"
			default="0"/>

		<integer-field
			name="outSent"
			default="0"/>

		<integer-field
			name="outSubmitted"
			default="0"/>

		<integer-field
			name="outDelivered"
			default="0"/>

		<integer-field
			name="outUndelivered"
			default="0"/>

		<integer-field
			name="outReportTimedOut"
			default="0"/>

		<integer-field
			name="outHeld"
			default="0"/>

		<integer-field
			name="outBlacklisted"
			default="0"/>

		<integer-field
			name="outManuallyUndelivered"
			default="0"/>

		<integer-field
			name="outManuallyDelivered"
			default="0"/>

		<integer-field
			name="outRejected"
			default="0"/>

	</fields>

</record>
