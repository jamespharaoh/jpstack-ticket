<console-module
	name="router">

	<console-helper-provider
		object-name="router"
		view-priv="parent:">

		<context-stuff
			name="routerName"
			field="code"/>

		<priv-key
			name="router.manage"
			priv="parent:manage"/>

	</console-helper-provider>

	<object-context
		name="router"
		object-name="router">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="router"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
