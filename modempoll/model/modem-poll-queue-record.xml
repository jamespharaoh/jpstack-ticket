<record
	name="modemPollQueue"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- data -->

		<string-field
			name="pdu"/>

		<timestamp-field
			name="createdTime"
			column-type="postgresql"/>

		<!-- statistics -->

		<integer-field
			name="tries"
			default="0"/>

		<!-- state -->

		<timestamp-field
			name="retryTime"
			column-type="postgresql"/>

		<string-field
			name="error"/>

	</fields>

</record>
