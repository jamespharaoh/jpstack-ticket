<record
	name="mercatorProjection"
	type="major">

	<record-interfaces>

		<interface
			package="wbs.sms.locator.model"
			name="MercatorProjection"/>

	</record-interfaces>

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<code-field/>

		<!-- details -->

		<description-field/>

		<!-- data -->

		<reference-field
			type="biaxialEllipsoid"/>

		<floating-point-field
			name="scaleFactor"/>

		<floating-point-field
			name="originLongitude"/>

		<floating-point-field
			name="originLatitude"/>

		<floating-point-field
			name="originEasting"/>

		<floating-point-field
			name="originNorthing"/>

	</fields>

</record>