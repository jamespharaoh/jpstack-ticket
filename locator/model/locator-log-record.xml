<record
	name="locatorLog"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="locator"/>

		<timestamp-field
			name="startTime"
			column-type="postgresql"/>

		<!-- details -->

		<reference-field
			type="number"/>

		<!-- data -->

		<timestamp-field
			name="endTime"
			column-type="postgresql"/>

		<yes-no-field
			name="success"/>

		<longitude-latitude-field
			name="longLat"/>

		<reference-field
			type="service"/>

		<reference-field
			type="affiliate"/>

		<string-field
			name="errorCode"/>

		<reference-field
			name="errorText"
			type="text"/>

	</fields>

</record>