<console-module
	name="number-list-number">

	<console-helper-provider
		object-name="numberListNumber"
		view-priv="numberList:manage">

		<priv-key
			name="numberListNumber.manage"
			priv="numberList:manage"/>

		<context-stuff
			name="numberListId"
			field="numberList.id"/>

		<context-stuff
			name="numberListNumberName"
			field="number.number"/>

		<run-post-processor
			name="numberList"/>

	</console-helper-provider>

	<object-context
		name="numberListNumber"
		object-name="numberListNumber">

		<list>

			<context-tab-action-page
				name="update"
				priv-key="numberList.manage"/>

			<object-search-page
				name="search"
				object-type="numberListNumber"
				search-class="wbs.sms.number.list.model.NumberListNumberSearch"
				search-form="search"
				results-form="results"
				priv-key="numberList.manage"/>

			<context-link
				name="history"
				link="numberListUpdate"
				label="History"
				priv-key="numberList.manage"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="search"
			class="wbs.sms.number.list.model.NumberListNumberSearch"
			type="search"/>

		<form
			name="results"
			object-type="number-list-number"
			type="read-only">

			<columns>

				<object-field
					name="number"
					object-type="number"/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="number-list-number"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>

				<object-field
					name="number"
					object-type="number"/>

				<yes-no-field
					name="present"/>

			</columns>

		</form>

	</forms>

</console-module>