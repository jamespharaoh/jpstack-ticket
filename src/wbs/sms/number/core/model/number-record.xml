<record
	name="number"
	type="common">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<code-field
			name="number"/>

		<!-- state -->

		<reference-field
			type="network"/>

		<timestamp-field
			name="archiveDate"
			column-type="postgresql"
			nullable="yes"/>

		<yes-no-field
			name="free"
			default="no"/>

	</fields>

	<event-types>

		<event-type
			name="Number network from message"
			text="Network for %0 changed from %1 to %2 due to %3"
			admin="no"/>

	</event-types>

</record>