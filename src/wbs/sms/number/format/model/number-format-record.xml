<record
	name="numberFormat"
	type="major">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<code-field/>

		<!-- details -->

		<name-field/>

		<description-field/>

		<!-- statistics -->

		<integer-field
			name="numPatterns"
			default="0"/>

	</fields>

	<collections>

		<children-list
			type="numberFormatPattern"/>

	</collections>

</record>