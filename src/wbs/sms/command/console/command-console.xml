<console-module
	name="command">

	<console-helper-provider
		object-name="command"
		view-priv="parent:">

		<priv-key
			name="command.manage"
			priv="parent.manage"/>

		<context-stuff
			name="commandName"
			field="code"/>

	</console-helper-provider>

	<object-context
		name="command"
		object-name="command">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="command"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
