<record
	name="inboxMultipartBuffer"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<!-- data -->

		<reference-field
			type="route"/>

		<integer-field
			name="multipartId"/>

		<integer-field
			name="multipartSegMax"/>

		<integer-field
			name="multipartSeg"/>

		<string-field
			name="msgTo"/>

		<string-field
			name="msgFrom"/>

		<timestamp-field
			name="msgNetworkTime"
			column-type="postgresql"/>

		<reference-field
			name="msgNetwork"
			type="network"/>

		<string-field
			name="msgOtherId"/>

		<string-field
			name="msgText"/>

	</fields>

</record>