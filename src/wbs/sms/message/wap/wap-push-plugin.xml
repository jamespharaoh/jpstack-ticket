<plugin
	name="wap-push"
	package="wbs.sms.message.wap">

	<models>
		<model name="wapPushMessage"/>
	</models>

	<layer name="logic">
		<component class="logic.WapPushLogic"/>
	</layer>

	<layer name="console">
		<component class="console.WapPushConsolePlugin"/>
	</layer>

	<console-modules>
		<console-module name="wap-push-message"/>
	</console-modules>

</plugin>
